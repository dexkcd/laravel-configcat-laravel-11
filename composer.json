{
    "name": "dexkcd/laravel-configcat-laravel-11",
    "description": "Implement feature flags with ConfigCat cloud service",
    "keywords": [
        "pod-point",
        "laravel",
        "configcat",
        "feature",
        "flags"
    ],
    "homepage": "https://github.com/dexkcd/laravel-configcat-laravel-11",
    "license": "MIT",
    "authors": [
        {
            "name": "Dexter Edep",
            "email": "dexter.edep@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "configcat/configcat-client": "^9.0",
        "illuminate/contracts": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6.7"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "PodPoint\\ConfigCat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PodPoint\\ConfigCat\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PodPoint\\ConfigCat\\ConfigCatServiceProvider"
            ],
            "aliases": {
                "ConfigCat": "PodPoint\\ConfigCat\\Facades\\ConfigCat"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
